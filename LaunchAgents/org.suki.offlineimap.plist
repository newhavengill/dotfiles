<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.suki.offlineiamp</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/offlineimap</string>
    </array>

    <key>RunAtLoad</key>
    <true></true>

    <key>StartInterval</key>
    <integer>60</integer>

    <key>TimeOut</key>
    <integer>180</integer>

    <key>NetworkState</key>
    <true></true>

    <key>Nice</key>
    <integer>15</integer>

    <key>ProcessType</key>
    <string>Background</string>

    <key>StandardErrorPath</key>
    <string>/Users/abhinav/log/offlineimap_abhinav_error.log</string>

    <key>StandardOutPath</key>
    <string>/Users/abhinav/log/offlineimap_abhinav.log</string>
  </dict>
</plist>
