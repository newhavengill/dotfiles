# Just for fun some common typos.
alias lsd="echo 'Wow, pretty colors, taste yummy.'; ls"
alias sl="echo 'Are you dyslexic?'; ls"
alias xs="echo 'Have you gone mad?'; cd"
alias ks="echo 'Take a typing lesson or something!'; ls"
alias vf="echo 'Ha Ha, Very Funny.'; cd"
alias csd="echo 'OK, I get the idea, you can not type!'; cd"
alias cds="echo 'Please try to type like a SANE person!'; cd"

